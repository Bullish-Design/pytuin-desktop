    type: editor
    props:
      name: tui
      code: "# textual_app.py\n# /// script\n# requires-python = \">=3.11\"\n# dependencies\
        \ = [\n#     \"textual>=0.47.0\",\n# ]\n# ///\n\n# Testing update from collapsed\
        \ block\n\nfrom __future__ import annotations\n\nfrom textual.app import App,\
        \ ComposeResult\nfrom textual.containers import Container, Horizontal\nfrom\
        \ textual.widgets import Button, Footer, Header, Label, TabbedContent, TabPane\n\
        \n\nclass MenuBar(Container):\n    \"\"\"Menu bar with action buttons.\"\"\
        \"\n\n    def compose(self) -> ComposeResult:\n        with Horizontal():\n\
        \            yield Button(\"Settings\", id=\"settings\", variant=\"primary\"\
        )\n            yield Button(\"Help\", id=\"help\")\n            yield Button(\"\
        Exit\", id=\"exit\", variant=\"error\")\n\n\nclass TUIApp(App):\n    \"\"\"\
        A simple TUI application with menu and tabs.\"\"\"\n\n    CSS = \"\"\"\n \
        \   MenuBar {\n        dock: top;\n        height: 3;\n        background:\
        \ $panel;\n    }\n\n    MenuBar Horizontal {\n        height: 1;\n       \
        \ padding: 1;\n    }\n\n    TabPane {\n        padding: 1;\n    }\n    \"\"\
        \"\n\n    BINDINGS = [\n        (\"q\", \"quit\", \"Quit\"),\n    ]\n\n  \
        \  def compose(self) -> ComposeResult:\n        yield Header()\n        yield\
        \ MenuBar()\n        with TabbedContent(initial=\"home\"):\n            with\
        \ TabPane(\"Home\", id=\"home\"):\n                yield Label(\"Welcome to\
        \ the home tab\")\n                yield Label(\"This is a simple textual\
        \ TUI application\")\n            with TabPane(\"Dashboard\", id=\"dashboard\"\
        ):\n                yield Label(\"Dashboard Content\")\n                yield\
        \ Label(\"Data and metrics would go here\")\n            with TabPane(\"About\"\
        , id=\"about\"):\n                yield Label(\"About This App\")\n      \
        \          yield Label(\"Built with Textual\")\n        yield Footer()\n\n\
        \    def on_button_pressed(self, event: Button.Pressed) -> None:\n       \
        \ button_id = event.button.id\n        if button_id == \"exit\":\n       \
        \     self.exit()\n        elif button_id == \"settings\":\n            self.notify(\"\
        Settings clicked\")\n        elif button_id == \"help\":\n            self.notify(\"\
        Help clicked\")\n\n\nif __name__ == \"__main__\":\n    app = TUIApp()\n  \
        \  app.run()"
      language: python
      variableName: Textual_App
      syncVariable: false
    children: []
